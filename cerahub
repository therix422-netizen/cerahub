local function openGUI()
    cube.Visible = false
    frame.Size = UDim2.new(0, 0, 0, 0)
    frame.BackgroundTransparency = 1
    frame.Visible = true
    TweenService:Create(frame, TweenInfo.new(0.5, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
        Size = UDim2.new(0, 550, 0, 330),
        BackgroundTransparency = 0.1
    }):Play()
end

local function closeGUI()
    local tween = TweenService:Create(frame, TweenInfo.new(0.4, Enum.EasingStyle.Quint, Enum.EasingDirection.In), {
        Size = UDim2.new(0, 0, 0, 0),
        BackgroundTransparency = 1
    })
    tween:Play()
    tween.Completed:Connect(function()
        frame.Visible = false
        cube.Visible = true
    end)
end

closeBtn.MouseButton1Click:Connect(closeGUI)
cube.MouseButton1Click:Connect(openGUI)

openGUI()
